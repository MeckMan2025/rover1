# NTRIP Credentials for GPS RTK Corrections
# Copy this file to .env and fill in your actual credentials
# DO NOT commit the .env file to git!

NTRIP_USER=your_username_here
NTRIP_PASS=your_password_here
NTRIP_HOST=165.206.203.10
NTRIP_MOUNTPOINT=RTCM3_IMAX
NTRIP_CASTER_PORT=10000

# Rover Login
ROVER_USER=your_rover_user
ROVER_PASSWORD=your_rover_password

# WiFi Networks (Priority Order)
# Home network - highest priority, connects when available
WIFI_HOME_SSID=your_home_ssid
WIFI_HOME_PASS=your_home_password

# Mobile hotspot - fallback when home network unavailable
WIFI_HOTSPOT_SSID=your_hotspot_ssid
WIFI_HOTSPOT_PASS=your_hotspot_password
